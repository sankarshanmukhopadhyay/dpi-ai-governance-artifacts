{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Supply Chain Control",
  "type": "object",
  "required": ["component_id", "dependency_type", "version"],
  "properties": {
    "component_id": { "type": "string" },
    "dependency_type": { "type": "string", "enum": ["model", "library", "api", "infrastructure"] },
    "version": { "type": "string" },
    "integrity_hash": { "type": "string" },
    "provenance_ref": { "type": "string" },
    "risk_classification": { "type": "string" },
    "last_verified": { "type": "string", "format": "date-time" }
  },
  "additionalProperties": false
}